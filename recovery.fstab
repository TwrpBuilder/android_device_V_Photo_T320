/boot		emmc	/dev/block/bootdevice/by-name/boot
/recovery	emmc	/dev/block/bootdevice/by-name/recovery	flags=backup=1
/system		ext4	/dev/block/bootdevice/by-name/system
/data		ext4	/dev/block/bootdevice/by-name/userdata	flags=encryptable=/dev/block/bootdevice/by-name/metadata
/cache		ext4	/dev/block/bootdevice/by-name/cache
/misc		emmc	/dev/block/bootdevice/by-name/misc
/metadata   emmc    /dev/block/bootdevice/by-name/metadata
/external_sd	vfat		/dev/block/platform/mtk-msdc.0/11240000.msdc1/mmcblk1p1	/dev/block/platform/mtk-msdc.0/11240000.msdc1/mmcblk1 flags=display="External SDcard";storage;wipeingui;removable;backup=0;encryptable=userdata
/usb-otg        vfat     	/dev/block/sda1 /dev/block/sda flags=display="USB OTG";storage;wipeingui;removable
